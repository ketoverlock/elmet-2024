{
	"version": "2",
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"border": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true
		},
		"color": {
			"background": true,
			"custom": true,
			"duotone": [],
			"customDuotone": false,
			"defaultDuotone": false,
			"customGradient": true,
			"defaultGradients": false,
			"gradients": [
				{
					"slug": "linear-blue",
					"gradient": "linear-gradient(-45deg,var(--wp--preset--color--et-navy),var(--wp--preset--color--et-blue),var(--wp--preset--color--et-navy))",
					"name": "Linear Blue"
				},
				{
					"slug": "linear-gray",
					"gradient": "linear-gradient(-45deg,var(--wp--preset--color--black),var(--wp--preset--color--et-dark-gray),var(--wp--preset--color--black))",
					"name": "Linear Gray"
				}
			],
			"defaultPalette": true,
			"link": true,
			"palette": [{
					"color": "#003057",
					"slug": "et-navy",
					"name": "Navy"
				},
				{
					"color": "#005F86",
					"slug": "et-blue",
					"name": "Blue"
				},
				{
					"color": "#FFC72C",
					"slug": "et-yellow",
					"name": "Yellow"
				},
				{
					"color": "#A0A0A0",
					"slug": "et-gray",
					"name": "Gray"
				},
				{
					"color": "#f5f3f5",
					"slug": "et-light-gray",
					"name": "Light Gray"
				},
				{
					"color": "#6E6E6E",
					"slug": "et-dark-gray",
					"name": "Dar Gray"
				}
			],
			"text": true
		},
		"shadow": {
			"presets": [
				{
					"name": "Standard",
					"slug": "standard",
					"shadow": "0px 5px 10px rgba(0, 0, 0, .2)"
				}
			]
		},
		"custom": {
			"typography": {
				"lineHeight": {
					"tiny": 1,
					"small": 1.2,
					"medium": 1.6,
					"normal": 1.8
				}
			}
		},
		"layout": {
			"contentSize": "min(950px, 90%)",
			"wideSize": "min(1920px, 90%)"
		},
		"spacing": {
			"blockGap": true,
			"margin": true,
			"padding": true,
			"spacingSizes": [{
					"size": "15px",
					"slug": "20",
					"name": "15px"
				},
				{
					"size": "clamp(15px, 3vw, 30px)",
					"slug": "30",
					"name": "30px"
				},
				{
					"size": "clamp(30px, 3vw, 45px)",
					"slug": "40",
					"name": "45px"
				},
				{
					"size": "clamp(30px, 3vw, 60px)",
					"slug": "50",
					"name": "60px"
				},
				{
					"size": "clamp(30px, 5vw, 75px)",
					"slug": "60",
					"name": "75px"
				},
				{
					"size": "clamp(45px, 7vw, 90px)",
					"slug": "70",
					"name": "90px"
				},
				{
					"size": "clamp(60px, 7vw, 120px)",
					"slug": "80",
					"name": "120px"
				}
			],
			"units": [
				"px",
				"em",
				"rem",
				"vh",
				"vw",
				"%"
			]
		},
		"typography": {
			"customFontSize": true,
			"dropCap": false,
			"fontFamilies": [{
					"fontFamily": "\"azo-mono\", monospace",
					"name": "Azo Mono",
					"slug": "azo-mono"
				},
				{
					"fontFamily": "\"azo-sans-web\", sans-serif",
					"name": "Azo Sans",
					"slug": "azo-sans"
				},
				{
					"fontFamily": "\"proxima-nova\", sans-serif",
					"name": "Proxima Nova",
					"slug": "proxima-nova"
				}
			],
			"fontSizes": [{
					"name": "Small",
					"size": "clamp(1.4rem, 3vw, 1.6rem)",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "clamp(1.8rem, 3vw, 2rem)",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "clamp(2rem, 3vw, 2.8rem)",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "clamp(2rem, 3vw, 3.8rem)",
					"slug": "x-large"
				},
				{
					"name": "XX Large",
					"size": "clamp(2.8rem, 3vw, 4.2rem)",
					"slug": "xx-large"
				}

			],
			"fontStyle": true,
			"fontWeight": true,
			"letterSpacing": true,
			"lineHeight": true,
			"textDecoration": true,
			"textTransform": true
		},
		"border": {
			"customColor": true,
			"customRadius": true,
			"customStyle": true,
			"customWidth": true
		},
		"blocks": {
			"core/heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--azo-sans)",
					"fontWeight": "700",
					"lineHeight": "1",
					"fontSizes": [{
							"name": "Small",
							"size": "2.4rem",
							"slug": "small"
						},
						{
							"name": "Medium",
							"size": "clamp(2.4rem, 6vw, 4rem)",
							"slug": "medium"
						},
						{
							"name": "Large",
							"size": "clamp(2.8rem, 8vw, 5rem)",
							"slug": "large"
						},
						{
							"name": "Extra Large",
							"size": "clamp(4rem, 8vw, 7.5rem)",
							"slug": "x-large"
						},
						{
							"name": "XX Large",
							"size": "clamp(6rem, 8vw, 10rem)",
							"slug": "xx-large"
						}
					]
				}
			},
			"core/buttons": {
				"spacing": {
					"blockGap": "var(--wp--preset--spacing--20)"
				}
			},
			"core/columns": {
				"spacing": {
					"blockGap": "var(--wp--preset--spacing--50)"
				}
			}
		}
	},
	"styles": {
		"border": {
			"color": "",
			"radius": "",
			"style": "",
			"width": ""
		},
		"color": {
			"background": "var(--wp--preset--color--et-light-gray)",
			"gradient": "",
			"text": "var(--wp--preset--color--et-dark-gray)"
		},
		"filter": {
			"duotone": ""
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--proxima-nova)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontStyle": "",
			"fontWeight": "400",
			"letterSpacing": "",
			"lineHeight": "1.5",
			"textDecoration": "",
			"textTransform": ""
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--30)"
		},
		"elements": {
			"link": {
				"border": {},
				"color": {
					"text": "var(--wp--preset--color--et-blue)"
				},
				"spacing": {},
				"typography": {
					"textDecoration": "none"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--et-teal)"
					},
					"typography": {
						"textDecoration": "underline"
					}
				},
				":focus": {
					"color": {
						"text": "var(--wp--preset--color--et-teal)"
					},
					"typography": {
						"textDecoration": "underline"
					}
				}
			},
			"button": {
				"spacing": {
					"padding": "15px var(--wp--preset--spacing--40)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--azo-sans)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "700",
					"lineHeight": "1",
					"textAlign": "center",
					"textTransform": "uppercase",
					"letterSpacing": ".1em"
				},
				"color": {
					"gradient": "var(--wp--preset--gradient--linear-blue)",
					"text": "var(--wp--preset--color--white)"
				},
				"border": {
					"radius": "0",
					"width": "0px",
					"style": "solid",
					"color": "var(--wp--preset--color--et-blue)"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--et-navy)",
						"text": "var(--wp--preset--color--white)"
					},
					"border": {
						"color": "var(--wp--preset--color--et-navy)"
					}
				},
				":focus": {
					"color": {
						"background": "var(--wp--preset--color--et-navy)",
						"text": "var(--wp--preset--color--white)"
					},
					"border": {
						"color": "var(--wp--preset--color--et-navy)"
					}
				}
			},
			"heading": {
				"color": {
					"text": "var(--wp--preset--color--et-blue)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--azo-sans)",
					"fontWeight": "700",
					"lineHeight": "1",
					"textTransform": "uppercase",
					"letterSpacing": ".1em"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "500"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			}
		}
	},
